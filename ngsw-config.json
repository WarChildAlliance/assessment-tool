{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "assetGroups": [
    {
      "installMode": "prefetch",
      "name": "app",
      "resources": {
        "files": [
          "/index.html",
          "/manifest.webmanifest",
          "/media/*",
          "/*.scss",
          "/*.css",
          "/*.js"
        ]
      },
      "updateMode": "prefetch"
    },
    {
      "installMode": "prefetch",
      "name": "assets",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      },
      "updateMode": "prefetch"
    }
  ],
  "dataGroups": [
    {
      "cacheConfig": {
        "maxAge": "30d",
        "maxSize": 100,
        "strategy": "freshness",
        "timeout": "0u"
      },
      "name": "api-queries",
      "urls": [
        "http://localhost:8002",
        "https://glmt-api.development.humanitarian.tech"
      ]
    }
  ],
  "index": "/index.html"
}
