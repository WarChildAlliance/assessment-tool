{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "http://localhost:8002/**",
        "https://glmt-api.development.humanitarian.tech/**",
        "https://api.gobee-education.org/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "2d",
        "timeout": "5s"
      }
    }
  ],
  "assetGroups": [
    {
      "installMode": "prefetch",
      "name": "app",
      "resources": {
        "files": [
          "/index.html",
          "/manifest.webmanifest",
          "/media/*",
          "/*.scss",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://glmt-api.development.humanitarian.tech/media/*",
          "http://localhost:8002/media/*",
          "https://api.gobee-education.org/media/*"
        ]
      },
      "updateMode": "prefetch"
    },
    {
      "installMode": "prefetch",
      "name": "assets",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      },
      "updateMode": "prefetch"
    }
  ],
  "index": "/index.html"
}
